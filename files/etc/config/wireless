config wifi-device 'radio0'
	option type 'mac80211'
	option channel 'auto'
	option band '2g'
	option htmode 'HT20'
	option disabled '0'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Test'
	option encryption 'none'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel 'auto'
	option band '5g'
	option htmode 'VHT80'
	option disabled '0'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Test5G'
	option encryption 'none'
